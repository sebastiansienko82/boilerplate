{
  "name": "lda-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "concurrently \"yarn run server:build\" \"yarn run client:build\"",
    "start": "concurrently \"yarn run server:start\" \"yarn run client:start\"",
    "prod": "node build/index.js",
    "client:install": "cd ./client && yarn",
    "client:start": "cd ./client && PORT=3001 yarn start",
    "server:start": "nodemon",
    "client:build": "cd ./client && yarn build",
    "server:build": "rimraf ./build && tsc",
    "test": "jest --runInBand",
    "lint": "eslint --config=./.eslintrc.js './**/*.{ts,tsx}'",
    "pre-commit": "yarn lint && npm run test",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.6.4",
    "concurrently": "7.4.0",
    "express": "4.18.1",
    "fs": "0.0.2",
    "https": "1.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "dotenv": "16.0.3",
    "eslint": "8.21.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.7"
  }
}
